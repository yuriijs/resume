<main>
  {{#with page_header}}
    <h1 class='text-center'>{{title}}</h1>
    <p class='text-center'>{{description}}</p>
  {{/with}}

  {{#with section}}
    {{#with about_us}}
      {{#with title_list}}
        <div class='display-6 text-end'>{{main}}</div>
        <h2 class='text-end'>{{sub}}</h2>
      {{/with}}
      <button type="button" class="btn btn-outline-info">{{button.text}}</button>
    {{/with}}
    <hr />
    <br />
    {{#with advantage}}
      <div class='display-5 text-muted'>{{title}}</div>
      <section>
        <div class='card'>
          <div class='card-body'>
            {{#with efficiency}}
              <div class='display-4 text-muted'>{{title}}</div>
              <h6>{{sub_title}}</h6>
              <p>{{description}}</p>
              <button type="button" class="btn btn-outline-warning">{{button}}</button>
            {{/with}}
          </div>
        </div>
        <br />
        <div class='card'>
          <div class='card-body'>
            {{#with innovation}}
              <div class='display-4 text-center text-muted'>{{title}}</div>
              <h6 class='text-center'>{{sub_title}}</h6>
              <p class='text-center'>{{description}}</p>
              <button type="button" class="btn btn-outline-warning">{{button}}</button>
            {{/with}}
          </div>
        </div>
        <br />
        <div class='card'>
          <div class='card-body'>
            {{#with collaboration}}
              <div class='display-4 text-end text-muted'>{{title}}</div>
              <h6 class='text-end'>{{sub_title}}</h6>
              <p class='text-end'>{{description}}</p>
              <button type="button" class="btn btn-outline-warning">{{button}}</button>
            {{/with}}
          </div>
        </div>
      </section>
    {{/with}}
    <br />
    <hr />
    <section>
      {{#with agreement}}
        <div class='display-5'>{{title}}</div>
        <br />
        <div class='card'>
          <div class='card-body'>
            {{#with info}}
              <h6>{{sub_title}}</h6>
              <hr />
              <section>
                {{#with detail}}
                  <p>{{text}}</p>
                  <button type="button" class="btn btn-success">{{button}}</button>
                {{/with}}
              </section>
            {{/with}}
          </div>
        </div>
      {{/with}}
    </section>
  {{/with}}
  


  
</main>